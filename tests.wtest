import rutina.*

describe "tests 1" {
    test "running en la playa" {
        const rutinaTest = new Running(intensidad = 1.2)
        assert.equals(360, rutinaTest.caloriasGastadas(5))
        assert.equals(10200, rutinaTest.caloriasGastadas(90))
    }

    test "maraton de BA"{
        const maratonBA = new Maraton(intensidad = 1.5)
        assert.equals(900, maratonBA.caloriasGastadas(5))
        assert.equals(25500, maratonBA.caloriasGastadas(90))
    }

    test "remo en el delta"{
        const remo = new Remo()
        assert.equals(520, remo.caloriasGastadas(5))
        assert.equals(9360, remo.caloriasGastadas(90))
    }
 
    test "remar en el camino" {
        const remoCompetitivo =  new RemoCompetitivo()
        assert.equals(510, remoCompetitivo.caloriasGastadas(5))
        assert.equals(12750, remoCompetitivo.caloriasGastadas(90))
    }

}
